{
    "env": {
        "esm": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": ">0.5%, not dead, not op_mini all, chrome 71",
                        "modules": false
                    }
                ],
                "@babel/preset-react",
                "@babel/preset-typescript"
            ]
        },
        "cjs": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": { "node": 12 },
                        "modules": "commonjs"
                    }
                ],
                "@babel/preset-react",
                "@babel/preset-typescript"
            ]
        }
    }
}
